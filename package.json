{
  "name": "Classroom-Manager",
  "version": "1.0.0",
  "description": "Classroom Manager",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "author": "Matheusky",
  "license": "MIT",
  "build": {
    "appId": "Classroom Manager",
    "asar": "false",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/img/icon.png"
    },
    "nsis": {
      "license": "LICENSE",
      "oneClick": "false",
      "allowToChangeInstallationDirectory": "true"
    }
  },
  "devDependencies": {
    "electron": ">=11.5.0",
    "electron-builder": "^22.10.5"
  },
  "dependencies": {
    "convert-csv-to-json": "0.0.15",
    "csvjson": "^5.1.0",
    "googleapis": "^59.0.0",
    "xlsx": ">=0.17.0"
  }
}
